sudo su postgres sh -c "echo 'DROP DATABASE openstreetmap;' | psql"
sudo su postgres createdb -E UTF8 -O openstreetmap openstreetmap
sudo su postgres psql -d openstreetmap < /usr/share/postgresql/8.4/contrib/btree_gist.sql
